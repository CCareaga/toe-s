/* The bootloader will look at this image and start execution at the symbol
   designated as the entry point. */
ENTRY(load_kern)

SECTIONS
{

	. = 0x2000;

	.text BLOCK(4K) : ALIGN(4K)
	{
		*(.text)
	}

	.rodata BLOCK(4K) : ALIGN(4K)
	{
		*(.rodata)
	}

	.data BLOCK(4K) : ALIGN(4K)
	{
		*(.data)
	}

	.bss BLOCK(4K) : ALIGN(4K)
	{
		*(.bss)
	}
}